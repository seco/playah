{
  "name": "playah",
  "version": "0.0.0",
  "description": "Helps draw video on canvas",
  "keywords": [
    "video",
    "html5",
    "canvas"
  ],
  "main": "index.js",
  "homepage": "https://github.com/thewhodidthis/playah#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/playah.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/playah/issues"
  },
  "config": {
    "remote": "thewhodidthis@zazie:www/example/playah/"
  },
  "scripts": {
    "test": "browserify test | testling -x open",
    "lint": "eslint . example --ext .es test/*.js",
    "start": "rollup -c && uglifyjs dist/playah.js -cmo dist/playah.min.js",
    "example": "npm run example:build && npm run example:serve",
    "example:build": "rollup -c -i example/index.es -f iife -o example/index.js",
    "example:serve": "php -S localhost:8000 -t example/",
    "example:press": "rsync -avruzLPC --delete --exclude=.DS_Store ./example/ $npm_package_config_remote"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.3"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "latest",
    "babel-plugin-external-helpers": "latest",
    "babel-preset-latest": "latest",
    "browserify": "latest",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "latest",
    "rollup": "latest",
    "rollup-plugin-babel": "latest",
    "tape": "latest",
    "testling": "latest",
    "uglify-js": "latest"
  },
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "MIT"
}
